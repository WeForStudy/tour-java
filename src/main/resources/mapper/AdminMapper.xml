    <insert id="insertNormal"  parameterType="com.tour.group.twow.entity.Admin" >
        INSERT INTO tour_admin
        <trim prefix="(" suffix=")" suffixOverrides=",">
            phone,password,
            <if test="name != null">
                name,
            </if>
            <if test="state != null">
                state,
            </if>
        </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=",">
            #{phone},#{password},
            <if test="name != null">
                #{name},
            </if>
            <if test="state != null">
                #{state}
            </if>
        </trim>
    </insert>

    <select id="isExist" resultType="com.tour.group.twow.entity.Admin" parameterType="com.tour.group.twow.entity.Admin">
        SELECT *
        FROM tour_admin
        <trim prefix="WHERE">
            <if test="account != null">
                account = #{account}
            </if>
            <if test="password != null and password.length()>0">
                AND password = #{password}
            </if>
        </trim>
    </select>
    <update id="update" parameterType="com.tour.group.twow.entity.Admin">
        UPDATE tour_admin
        <trim prefix="SET" suffixOverrides=",">
            <if test="phone != null and phone.length() == 11">
                phone = #{phone},
            </if>
            <if test="account != null and account.length() == 11">
                account = #{account},
            </if>
            <if test="password != null and password.length() >0">
                password = #{password},
            </if>
            <if test="name != null and name.length() > 0">
                name = #{name},
            </if>
            <if test="status != null ">
                status = #{status},
            </if>
            <if test="type != null and type != 0 ">
                type = #{type},
            </if>
            <if test="extraInfo != null and extraInfo.length() > 0">
                extra_info = #{extraInfo},
            </if>
        </trim>
        WHERE  id = #{id}

    </update>
    <select id="selectAll" resultType="com.tour.group.twow.entity.Admin">
        SELECT *
        FROM tour_admin
        WHERE status != 404
    </select>

